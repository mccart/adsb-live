{
  "name": "adsb-live",
  "version": "1.0.0",
  "description": "Javascript client and Apollo server for worldwide aircraft locations provided by ADSB Exchange",
  "main": "src/index.js",
  "repository": {
    "url": "git@github.com:hemerajs/modern-node-package.git",
    "type": "git"
  },
  "scripts": {
    "precommit": "pretty-quick --staged && npm run lint && npm run test",
    "lint": "eslint \"./src/**/*.js\" \"./test/**/*.js\"",
    "lint:fix": "eslint --fix  \"./src/**/*.js\" \"./test/**/*.js\"",
    "format": "pretty-quick",
    "test": "npm run lint && ava",
    "test:watch": "ava -w",
    "coverage": "nyc ava",
    "coverage:html": "nyc ava && nyc report --reporter=html",
    "coveralls": "npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "adsb",
    "apollo"
  ],
  "author": "Ryan Festag",
  "license": "ISC",
  "dependencies": {
    "oboe": "^2.1.4",
    "rxjs": "^6.2.2",
    "rxjs-compat": "^6.2.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "coveralls": "^3.0.1",
    "eslint-config-hemera": "^0.0.6",
    "husky": "^0.14.3",
    "np": "^3.0.1",
    "nyc": "^11.8.0",
    "prettier-standard": "^8.0.1",
    "pretty-quick": "^1.6.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1"
  }
}
